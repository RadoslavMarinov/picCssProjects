#include <18F4550.h>
#fuses HSPLL, PLL5, USBDIV, NOIESO, PUT, NOWDT, NOPBADEN, MCLR, STVREN, NODEBUG, PROTECT
#use delay (crystal = 48M)
#define Y_LED PIN_D1
#define G_LED PIN_D0
#define R_LED PIN_C2

#include <usb_cdc.h>

void toggle_led(const int);

void main (void)
{
delay_ms(100);
usb_init();
toggle_led(G_LED);

}

void toggle_led(const int led)
{
output_toggle(led);
delay_ms(500);
}
